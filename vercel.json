{
  "version": 2,
  "name": "human-first-seo",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai-api-key",
    "ANTHROPIC_API_KEY": "@anthropic-api-key",
    "GOOGLE_AI_API_KEY": "@google-ai-api-key"
  },
  "functions": {
    "app/api/competitor/analyze/route.ts": {
      "maxDuration": 30
    },
    "app/api/content/analyze/route.ts": {
      "maxDuration": 45
    }
  }
}
